{
  "name": "electron-quick-start",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "pack:packager": "electron-packager . app --overwrite --platform win32 --arch=x64 --asar=true --out dist/",
    "pack:installer": "electron-installer-windows --src dist/app-win32-x64/ --dest dist/installers/"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^9.2.0",
    "electron-builder": "^22.8.0",
    "electron-installer-windows": "^3.0.0",
    "electron-packager": "^15.0.0",
    "file-download": "^0.1.2"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "dns": "^0.2.2",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "http": "0.0.1-security",
    "internet-available": "^1.0.0",
    "is-online": "^8.4.0",
    "js-file-downloader": "^1.1.10",
    "quick-local-ip": "^1.0.7",
    "request": "^2.88.2",
    "request-progress": "^3.0.0",
    "rxjs": "^6.6.2",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "ws": "^7.3.1"
  }
}
